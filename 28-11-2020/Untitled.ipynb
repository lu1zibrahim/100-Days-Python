   "source": [
    "def first_non_consecutive(arr):\n",
    "    i = 0\n",
    "    while i < len(arr)-1:\n",
    "        if arr[i] == arr[i+1]-1:\n",
    "            i+=1\n",
    "        else:\n",
    "            return arr[i+1]\n",
    "    return None"
   ]
